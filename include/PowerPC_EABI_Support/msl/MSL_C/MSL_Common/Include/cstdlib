#ifndef _MSL_CSTDLIB
#define _MSL_CSTDLIB

#include "null.h"
#include "size_t.h"

#ifdef __cplusplus
extern "C" {
#endif

#define abs(x) __abs(x)

double strtod(const char* nptr, char** endptr);

int rand(void);
void srand(unsigned int seed);

void* malloc(size_t);
void free(void*);
void* realloc(void*, size_t);

void abort(void); // FIXME: noreturn?

#ifdef __cplusplus
}
#endif

#endif
